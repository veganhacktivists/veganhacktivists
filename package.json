    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/luxon": "^3.0.0",
    "@types/mailchimp__mailchimp_marketing": "^3.0.5",
    "@types/node": "^18.0.6",
    "@types/react": "18.0.15",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-modal": "^3.13.1",
    "@types/react-syntax-highlighter": "^15.5.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.3",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jestSetup.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "<rootDir>/node_modules/babel-jest",
        {
          "presets": [
            "next/babel"
          ]
        }
      ]
    },
    "moduleNameMapper": {
      "\\.(eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/src/__mocks__/imageMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy"
    }
  },
  "optionalDependencies": {
    "contentful-management": "^10.8.0",
    "contentful-typescript-codegen": "^3.2.3"
  },
  "engines": {
    "pnpm": ">=7.0.0"
  }
}
