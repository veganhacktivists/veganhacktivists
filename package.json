    "@mailchimp/mailchimp_marketing": "^3.0.75",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "4.2.1",
    "@react-spring/web": "^9.5.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tanstack/react-query": "^4.2.1",
    "@trpc/client": "10.0.0-alpha.47",
    "@trpc/next": "10.0.0-alpha.47",
    "@trpc/react": "10.0.0-alpha.47",
    "@trpc/server": "10.0.0-alpha.47",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "contentful": "^9.1.32",
    "crypto-js": "^4.1.1",
    "discord.js": "^14.2.0",
    "form-data": "^4.0.0",
    "fuse.js": "^6.6.2",
    "http-status-codes": "^2.2.0",
    "luxon": "^3.0.1",
    "mailgun.js": "^7.0.4",
    "next": "12.2.2",
    "next-auth": "^4.10.3",
    "next-pwa": "^5.5.4",
    "next-seo": "^5.4.0",
    "nodemailer": "^6.7.7",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-gtm-module": "^2.0.11",
    "react-highlight-words": "^0.18.0",
    "react-hook-form": "^7.33.1",
    "react-modal": "^3.15.1",
    "react-qr-code": "^2.0.7",
    "react-select": "^5.4.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.0.6",
    "react-use-pagination": "^2.0.1",
    "react-waypoint": "^10.3.0",
    "sass": "^1.53.0",
    "sharp": "^0.30.7",
    "superjson": "^1.9.1",
