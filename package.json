{
  "name": "vh-site",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "postinstall": "(test -f ./scripts/postinstall.sh && ./scripts/postinstall.sh || true) && pnpm translation:compile",
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm translation:compile && NODE_OPTIONS='--inspect' next dev",
    "build": "pnpm translation:compile && next build",
    "start": "next start",
    "lint": "next lint --dir . --report-unused-disable-directives warn",
    "test": "jest --watch",
    "format": "prettier -w .",
    "contentful-typescript-codegen": "contentful-typescript-codegen --output src/types/generated/contentful.d.ts",
    "translation:extract": "ts-node translation/extract.ts && pnpm translation:compile",
    "translation:update": "ts-node translation/update.ts",
    "translation:compile": "ts-node translation/compile.ts",
    "translation:translate": "export $(cat .env | xargs) && ts-node translation/translate.ts && pnpm translation:compile"
  },
